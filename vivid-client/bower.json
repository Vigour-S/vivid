{
  "name": "vivid",
  "description": "bower components for vivid UI.",
  "dependencies": {
    "when": "~2",
    "meld": "~1",
    "most": "~0.2",
    "curl": "~0.8",
    "poly": "~0",
    "jquery": "1.8.3",
    "semantic": "~1.12",
    "gmaps": "0.4.14",
    "zeroclipboard": "1.1.7",
    "google-code-prettify": "~1.0",
    "jquery-timeago": "1.4.1"
  },
  "overrides": {
    "jquery": {
      "ignore": ["*.json"]
    },
    "curl": {
      "ignore": ["dist/**", "src/curl/**"]
    },
    "meld": {
      "ignore": ["aspect/**", "docs/**", "test/**"]
    },
    "most": {
      "ignore": ["test/**"]
    },
    "bootstrap": {
      "ignore": [
        "js/**", "img/**", "less/**", "docs/build/**",
        "docs/examples/**", "docs/templates/**", "docs/*.html"
      ]
    },
    "bootstrap-datetimepicker": {
      "ignore": ["build/**", "test/**"]
    },
    "font-awesome": {
      "ignore": ["css/**", "less/**", "scss/**", "src/**"]
    },
    "gmaps": {
      "ignore": ["examples/**", "test/**"]
    },
    "google-code-prettify": {
      "ignore": ["styles/**"]
    },
    "zeroclipboard": {
      "ignore": ["docs/**", "src/**", "test/**"]
    }
  }
}